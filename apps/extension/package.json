{
  "name": "espritads-portal",
  "displayName": "ESPRIT@ds Portal",
  "version": "1.0.0",
  "description": "Unofficial student portal extension for ESPRIT University. Access your grades, credits, schedules, and Blackboard integration in one place.",
  "author": "ESPRIT@ds Team",
  "homepage": "https://portal.espritads.site",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@lime1/esprit-ts": "1.1.2",
    "@supabase/supabase-js": "^2.49.8",
    "axios": "^1.13.2",
    "plasmo": "0.90.5",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@parcel/config-default": "2.9.3",
    "@parcel/resolver-default": "2.9.3",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "name": "ESPRIT@ds Portal",
    "short_name": "ESPRIT@ds",
    "host_permissions": [
      "https://esprit-tn.com/*",
      "https://*.esprit-tn.com/*",
      "https://esprit.blackboard.com/*",
      "https://portal.espritads.site/*",
      "https://app.espritads.site/*"
    ],
    "permissions": [
      "storage",
      "cookies",
      "webNavigation"
    ],
    "externally_connectable": {
      "matches": [
        "http://localhost:3000/*",
        "https://localhost:3000/*",
        "https://esprit-portal-v2.vercel.app/*",
        "https://portal.espritads.site/*",
        "https://*.espritads.site/*"
      ]
    },
    "commands": {
      "test": {
        "suggested_key": {
          "default": "Alt+W"
        },
        "description": "Open the extension main popup"
      }
    }
  }
}
