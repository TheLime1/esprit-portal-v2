{
  "name": "esprit-extension",
  "displayName": "Esprit Extension",
  "version": "0.0.1",
  "description": "Esprit Portal browser extension.",
  "author": "Esprit Team",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@lime1/esprit-ts": "1.1.2",
    "axios": "^1.13.2",
    "plasmo": "0.90.5",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@parcel/config-default": "2.9.3",
    "@parcel/resolver-default": "2.9.3",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "storage"
    ],
    "externally_connectable": {
      "matches": [
        "http://localhost:3000/*",
        "https://localhost:3000/*"
      ]
    },
    "commands": {
      "test": {
        "suggested_key": {
          "default": "Alt+W"
        },
        "description": "Open the extension main popup"
      }
    }
  }
}
